 private JFrame frame;
    private void jbtnExitActionPerformed(java.awt.event.ActionEvent evt) {                                         
       frame = new JFrame("Exit");
       if(JOptionPane.showConfirmDialog(frame, "Do you want to exit?","Exit",
               JOptionPane.YES_NO_OPTION)==JOptionPane.YES_OPTION) 
       {
           System.exit(0);
       }
       
    }                                        

    private void jtxtNameActionPerformed(java.awt.event.ActionEvent evt) {                                         
        // TODO add your handling code here:
    }                                        

    private void jbtnResetActionPerformed(java.awt.event.ActionEvent evt) {                                          
      jtxtName.setText("");
      jtxtPeripheralName.setText("");
      jtxtPeripheralType.setText("");
      jtxtId.setText("");
    }                                         

    private void jbtnAddActionPerformed(java.awt.event.ActionEvent evt) {                                        
      if(jtxtName.getText().equals("") || jtxtPeripheralName.getText().equals("") || jtxtPeripheralType.getText().equals("")
             || jtxtId.getText().equals("") ) {
      
          JOptionPane.showMessageDialog(this,"Please Enter all the data");
      }else{
      String data[] = {jtxtName.getText(),jtxtPeripheralName.getText(),jtxtPeripheralType.getText(),jtxtId.getText()} ;
     
      DefaultTableModel dt = (DefaultTableModel) jTable1.getModel();
      dt.addRow(data);
      JOptionPane.showMessageDialog(this,"Added Successfully");
      jtxtName.setText("");
      jtxtPeripheralName.setText("");
      jtxtPeripheralType.setText("");
      jtxtId.setText("");
      }
    }                                       

    private void jbtnDeleteActionPerformed(java.awt.event.ActionEvent evt) {                                           
        DefaultTableModel dt = (DefaultTableModel) jTable1.getModel();
        int selectedRows = jTable1.getSelectedRow();
        
        if (jTable1.getSelectedRowCount() == 1) {
        dt.removeRow(jTable1.getSelectedRow());
        
        JOptionPane.showMessageDialog(this,"Delete Successfully");
        } else{
        JOptionPane.showMessageDialog(this,"Please choice to Delete");}
    }                                          

    private void jbtnEditActionPerformed(java.awt.event.ActionEvent evt) {                                         
        // TODO add your handling code here:
    }                                        

    private void jTable1MouseClicked(java.awt.event.MouseEvent evt) {                                     
         DefaultTableModel dt = (DefaultTableModel) jTable1.getModel();
         int selectedRows = jTable1.getSelectedRow();
         
         String dtName = (String)dt.getValueAt(jTable1.getSelectedRow(), 1).toString();
         String dtPeripheralName = (String) dt.getValueAt(jTable1.getSelectedRow(), 2).toString();
         String dtPeripheralType = (String)dt.getValueAt(jTable1.getSelectedRow(), 3).toString();
         String dtId = (String) dt.getValueAt(jTable1.getSelectedRow(), 4).toString();
         
         jtxtName.setText(dtName);
         jtxtPeripheralName.setText(dtPeripheralName);
         jtxtPeripheralType.setText(dtPeripheralType);
         jtxtId.setText(dtId);
    }                                    

    private void jbtnEditMouseClicked(java.awt.event.MouseEvent evt) {                                      
        DefaultTableModel dt = (DefaultTableModel) jTable1.getModel();
        
        if (jTable1.getSelectedRowCount() == 1) {
            String name = jtxtName.getText();
            String peripheralname = jtxtPeripheralName.getText();
            String peripheraltype = jtxtPeripheralType.getText();
            String idcode = jtxtId.getText();
            
            dt.setValueAt(name, jTable1.getSelectedRow(), 0);
            dt.setValueAt(peripheralname, jTable1.getSelectedRow(), 1);
            dt.setValueAt(peripheraltype, jTable1.getSelectedRow(), 2);
            dt.setValueAt(idcode, jTable1.getSelectedRow(), 3);
            
            JOptionPane.showMessageDialog(this,"Update Successfully");
        }else{
            
        if(jTable1.getRowCount() == 0 ) {
            
           JOptionPane.showMessageDialog(this, "The Table is Empty!."); 
        }else{
                JOptionPane.showMessageDialog(this, "Please Select To Edit"); 
    }
        }
    }                                     

    private void jtxtIdActionPerformed(java.awt.event.ActionEvent evt) {                                       
        // TODO add your handling code here:
    }                                      

